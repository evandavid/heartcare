    <%= link_to "Back", :action => :index %>
    <h1><%= @session.user_id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @session.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">UserId</td>
        <td class="itemValue"><%= @session.user_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Token</td>
        <td class="itemValue"><%= @session.token%></td>
      </tr>
      
    </table>
  </div>

</div>
